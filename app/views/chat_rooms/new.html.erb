
<div class="container">
	<div class="row top-center-v-h">
		<div class="col-sm-6 col-sm-offset-3">
			<h1>Add Chat Room</h1>
			<%= form_for @chat_room do |f| %>
			  <div class="form-group">
			    <%= f.label :Chat_title %>
			    <%= f.text_field :title, autofocus: true, class: 'form-control' %>
			  </div>

			  <%= f.submit "Add!", class: 'btn btn-space btn-primary' %>
			<% end %>
			
		</div>
	</div>
</div>

